{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["plugin:vue/essential", "eslint:recommended"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "vue/no-unused-components": 0,
    "no-unused-vars": 0
  },
  "globals": {
    "_": true,
    "$api": true,
    "$auth": true,
    "$util": true,
    "$validate": true,
    "$is": true
  }
}
